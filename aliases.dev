#!/usr/bin/env bash

# Initial environment
# shellcheck source=/dev/null
export ASAPSHELL_DIR="$PWD"

# Helpers
source "$ASAPSHELL_DIR/src/helpers/index.sh"

# Config
source "$ASAPSHELL_DIR/src/config/index.sh"

# General aliases
source "$ASAPSHELL_DIR/src/general/index.sh"

### Get os name via uname ###
_myos="$(uname -s)"

### add alias as per os using $_myos ###
case $_myos in
Linux)
  # shellcheck source=/dev/null
  source "$ASAPSHELL_DIR/src/linux/index.sh"
  ;;
FreeBSD | OpenBSD)
  # shellcheck source=/dev/null
  source "$ASAPSHELL_DIR/src/bsd/index.sh"
  ;;
Darwin)
  # shellcheck source=/dev/null
  source "$ASAPSHELL_DIR/src/darwin/index.sh"
  ;;
*) ;;
esac
